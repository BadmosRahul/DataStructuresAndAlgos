class Solution {
public:
    vector<int> maxSlidingWindow(vector<int>& nums, int k) {
    deque<int> q;
    vector<int> t;
    
    
    for(int j = 0; j <= nums.size() - k;j++){
        int maxi = INT_MIN;
        int it = k;

        int i = j;
        while(it--){
            maxi = max(maxi,nums[i]);
            i++;
        }

     t.push_back(maxi);

    }
    
    return t;

    }
};
